{{~ensureProperties
        "security.webconsole.osgiManager.category"
        "security.webconsole.osgiManager.managerRoot"
        "security.webconsole.osgiManager.plugins"
        "security.webconsole.osgiManager.loglevel"
        "security.webconsole.osgiManager.username"
        "security.webconsole.osgiManager.realm"
        "security.webconsole.osgiManager.defaultRender"
        "security.webconsole.osgiManager.password"
~}}
[feature name=aem-cms-security-config]

[configurations]

  # Apache Felix OSGi Management Console configuration
  org.apache.felix.webconsole.internal.servlet.OsgiManager
  category="{{security.webconsole.osgiManager.category}}"
  manager.root="{{security.webconsole.osgiManager.managerRoot}}"
  plugins=(
  {{~#each security.webconsole.osgiManager.plugins ~}}
    "{{this}}"{{#unless @last}},{{/unless}},
  {{~/each ~}})
  loglevel=I"{{security.webconsole.osgiManager.loglevel}}"
  username="{{security.webconsole.osgiManager.username}}"
  http.service.filter="{{security.webconsole.osgiManager.httpServiceFilter}}"
  realm="{{security.webconsole.osgiManager.realm}}"
  default.render="{{security.webconsole.osgiManager.defaultRender}}"
  locale="{{security.webconsole.osgiManager.locale}}"
  password="{{webconsolePasswordHash security.webconsole.osgiManager.password}}"

